<section class="py-8">
  <h1 class="pt-4 pb-8 text-4xl font-medium">Book an appointment</h1>
  <h2 class="text-2xl font-medium">Available agents</h2>

  <ul class="xl:w-1/2 md:w-full">
    <% agents.each do |agent| %>
      <%= link_to agent_path(agent) do %>
        <%= render partial: "agent", locals: {agent:} %>
      <% end %>
    <% end %>
  </ul>
</section>
